           "languages": [
             	 {
     	         "lang": "nt",
        	      "name": "Подсказки на полях",
            	  "default": true
             	 },
            	  {
             	 "lang": "ex",
             	 "name": "Упражнения"
            	  }
              ],
           "split": true
            },
